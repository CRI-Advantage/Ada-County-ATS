            <choice>
                <element dependent_value="Canceled/Disqualified" label="Accepted another job offer" value="otherJob"/>
                <element dependent_value="Canceled/Disqualified" label="Application received too late" value="tooLate"/>
                <element dependent_value="Canceled/Disqualified" label="Does not meet minimum education requirement" value="education"/>
                <element dependent_value="Canceled/Disqualified" label="Does not meet minimum experience requirement" value="experience"/>
                <element dependent_value="Canceled/Disqualified" label="Failed background check" value="background"/>
                <element dependent_value="Canceled/Disqualified" label="Failed drug test" value="drug"/>
                <element dependent_value="Canceled/Disqualified" label="Failed to respond" value="failRespond"/>
                <element dependent_value="Canceled/Disqualified" label="More qualified candidate selected" value="qulCandidate"/>
                <element dependent_value="Canceled/Disqualified" label="Not willing/able to relocate" value="noRelocate"/>
